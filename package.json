{
  "author": "Tactile Vision",
  "name": "collaborative-tacton-frontend",
  "main": "dist_electron/background.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "dev": "vite",
    "lint": "eslint src",
    "lint:fix": "eslint src  --fix",
    "prettier": "prettier src",
    "prettier:fix": "prettier src  --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.0.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^29.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.24.1",
    "globals": "^15.0.0",
    "prettier": "3.2.5",
    "sass": "^1.74.1",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.6.0",
    "vite": "^5.2.8",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@abandonware/noble": "^1.9.2-24",
    "@mdi/font": "^7.4.47",
    "@pixi/canvas-renderer": "^7.4.2",
    "@pixi/settings": "^7.4.2",
    "@pixi/unsafe-eval": "^7.4.2",
    "electron-log": "^5.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.8",
    "pixi.js": "^8.1.0",
    "uuid": "^9.0.1",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.21",
    "vue-inline-svg": "^3.1.2",
    "vue-router": "^4.3.0",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.5.14",
    "vuex": "^4.1.0"
  },
  "build": {
    "appId": "edu.tactile-vision.collabjam-client",
    "productName": "Collaborative Tacton Generator",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "dist_electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  }
}
