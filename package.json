{
  "author": "Tactile Vision Research Group",
  "name": "collabjam-client",
  "main": "dist_electron/background.js",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": ">=20.12.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && vue-tsc --noEmit && npx vite build && npx electron-builder ",
    "lint": "eslint src",
    "lint:fix": "eslint src  --fix",
    "prettier": "prettier src",
    "prettier:fix": "prettier src  --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "clean": "rimraf dist dist_electron",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.0.0",
    "@nabla/vite-plugin-eslint": "^2.0.2",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^29.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^5.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.8",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.24.1",
    "eslint-plugin-vue-scoped-css": "^2.8.0",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "sass": "^1.74.1",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.6.0",
    "vite": "^5.2.8",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.13"
  },
  "dependencies": {
    "@abandonware/noble": "^1.9.2-24",
    "@mdi/font": "^7.4.47",
    "@pixi/canvas-renderer": "^7.4.2",
    "@pixi/settings": "^7.4.2",
    "@pixi/unsafe-eval": "^7.4.2",
    "pixi.js": "^8.1.0",
    "rimraf": "^5.0.7",
    "socket.io-client": "^4.7.5",
    "uuid": "^9.0.1",
    "vue": "^3.4.21",
    "vue-inline-svg": "^3.1.2",
    "vue-router": "^4.3.0",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.5.14",
    "vuex": "^4.1.0"
  },
  "build": {
    "appId": "edu.tactile-vision.collabjam-client",
    "productName": "CollabJam",
    "copyright": "Copyright Â© 2025 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "dist_electron/**/*"
    ],
    "directories": {
      "output": "dist_electron"
    }
  }
}